{
  "type": "column",
  "widgetWidth": 1,
  "widgetHeight": 2,
  "children": [
    {
      "type": "row",
      "flex": 2,
      "children": [
        {
          "type": "image_button",
          "id": "powered",
          "value": "$powered$",
          "padding": {
            "right": 15
          },
          "values": {
            "true": "/images/widgets/outlet_on.svg",
            "false": "/images/widgets/outlet_off.svg"
          }
        },
        {
          "type": "dropdown_button",
          "id": "source",
          "flex": 1,
          "isExpanded": true,
          "items": "$sourceValues$",
          "values": "$sources$",
          "value": "$source$"
        }
      ]
    },
    {
      "type": "row",
      "flex": 2,
      "crossAxisAlignment": 3,
      "children": [
        {
          "type": "raised_button",
          "id": "preset",
          "padding": {
            "right": 4,
            "bottom": 4
          },
          "flex": 3,
          "colorBrightness": 0,
          "text": "$preset1.itemName$",
          "value": 1
        },
        {
          "type": "raised_button",
          "id": "preset",
          "padding": {
            "right": 4,
            "left": 4,
            "bottom": 4
          },
          "flex": 3,
          "colorBrightness": 0,
          "text": "$preset2.itemName$",
          "value": 2
        },
        {
          "type": "raised_button",
          "name": "preset",
          "padding": {
            "left": 4,
            "bottom": 4
          },
          "flex": 3,
          "colorBrightness": 0,
          "text": "$preset3.itemName$",
          "value": 3
        }
      ]
    },
    {
      "type": "row",
      "flex": 2,
      "crossAxisAlignment": 3,
      "children": [
        {
          "type": "raised_button",
          "name": "preset",
          "text": "$preset4.itemName$",
          "flex": 3,
          "colorBrightness": 0,
          "padding": {
            "right": 4,
            "top": 4
          },
          "value": 4
        },
        {
          "type": "raised_button",
          "name": "preset",
          "flex": 3,
          "colorBrightness": 0,
          "padding": {
            "right": 4,
            "left": 4,
            "top": 4
          },
          "text": "$preset5.itemName$",
          "value": 5
        },
        {
          "type": "raised_button",
          "name": "preset",
          "flex": 3,
          "colorBrightness": 0,
          "padding": {
            "left": 4,
            "top": 4
          },
          "text": "$preset6.itemName$",
          "value": 6
        }
      ]
    },
    {
      "type": "row",
      "flex": 2,
      "children": [
        {
          "type": "image_button",
          "id": "previous",
          "flex": 1,
          "width": 30,
          "height": 30,
          "value": "/images/widgets/previous.svg"
        },
        {
          "type": "image_button",
          "flex": 1,
          "id": "playpause",
          "width": 30,
          "height": 30,
          "value": "$state$",
          "values": {
            "pause": "/images/widgets/pause.svg",
            "play": "/images/widgets/play.svg"
          }
        },
        {
          "type": "image_button",
          "id": "stop",
          "width": 30,
          "height": 30,
          "flex": 1,
          "value": "/images/widgets/stop.svg"
        },
        {
          "type": "image_button",
          "width": 30,
          "height": 30,
          "id": "next",
          "flex": 1,
          "value": "/images/widgets/next.svg"
        }
      ]
    },
    {
      "type": "slider",
      "id": "volume",
      "value": "$volume$",
      "min": 0,
      "max": 100,
      "divisions": 100,
      "flex": 1
    }
  ]
}
