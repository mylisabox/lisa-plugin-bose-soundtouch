{
  "type": "column",
  "widgetWidth": 1,
  "widgetHeight": 2,
  "children": [
    {
      "type": "row",
      "flex": 2,
      "children": [
        {
          "type": "image_button",
          "id": "power",
          "value": "$power$",
          "values": {
            "on": "/images/widgets/outlet_on.png",
            "off": "/images/widgets/outlet_off.png"
          }
        },
        {
          "type": "column",
          "children": [
            {
              "type": "text",
              "id": "power",
              "value": "$power$"
            }
          ]
        }
      ]
    },
    {
      "type": "row",
      "flex": 2,
      "crossAxisAlignment": 3,
      "children": [
        {
          "type": "raised_button",
          "id": "preset",
          "padding": {
            "right": 4,
            "bottom": 4
          },
          "flex": 3,
          "colorBrightness": 0,
          "text": "$preset1.itemName$",
          "value": 1
        },
        {
          "type": "raised_button",
          "id": "preset",
          "padding": {
            "right": 4,
            "left": 4,
            "bottom": 4
          },
          "flex": 3,
          "colorBrightness": 0,
          "text": "$preset2.itemName$",
          "value": 2
        },
        {
          "type": "raised_button",
          "name": "preset",
          "padding": {
            "left": 4,
            "bottom": 4
          },
          "flex": 3,
          "colorBrightness": 0,
          "text": "$preset3.itemName$",
          "value": 3
        }
      ]
    },
    {
      "type": "row",
      "flex": 2,
      "crossAxisAlignment": 3,
      "children": [
        {
          "type": "raised_button",
          "name": "preset",
          "text": "$preset4.itemName$",
          "flex": 3,
          "colorBrightness": 0,
          "padding": {
            "right": 4,
            "top": 4
          },
          "value": 4
        },
        {
          "type": "raised_button",
          "name": "preset",
          "flex": 3,
          "colorBrightness": 0,
          "padding": {
            "right": 4,
            "left": 4,
            "top": 4
          },
          "text": "$preset5.itemName$",
          "value": 5
        },
        {
          "type": "raised_button",
          "name": "preset",
          "flex": 3,
          "colorBrightness": 0,
          "padding": {
            "left": 4,
            "top": 4
          },
          "text": "$preset6.itemName$",
          "value": 6
        }
      ]
    },
    {
      "type": "row",
      "flex": 2,
      "children": [
        {
          "type": "image_button",
          "id": "previous",
          "flex": 1,
          "width": 30,
          "height": 30,
          "value": "/images/icons/media-step-backward.svg"
        },
        {
          "type": "image_button",
          "flex": 1,
          "id": "playpause",
          "width": 30,
          "height": 30,
          "value": "$state$",
          "values": {
            "pause": "/images/icons/media-pause.svg",
            "play": "/images/icons/media-play.svg"
          }
        },
        {
          "type": "image_button",
          "id": "stop",
          "width": 30,
          "height": 30,
          "flex": 1,
          "value": "/images/icons/media-stop.svg"
        },
        {
          "type": "image_button",
          "width": 30,
          "height": 30,
          "id": "next",
          "flex": 1,
          "value": "/images/icons/media-step-forward.svg"
        }
      ]
    },
    {
      "type": "slider",
      "id": "volume",
      "value": "$volume$",
      "min": 0,
      "max": 100,
      "divisions": 100,
      "flex": 1
    }
  ]
}
